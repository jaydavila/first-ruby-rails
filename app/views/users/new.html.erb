<h1>Sign up here</h1>
<p>Find me in app/views/users/new.html.erb</p>


<%= form_for @user do |f| %>
<div class="">
  <%= f.label :username, 'User Name' %>
    <%= f.text_field :username %>
  </div>
  <div>
    <%= f.label :fname %>
    <%= f.text_field :fname %>
  </div>
  <div>
      <%= f.submit %>
    </div>
<% end %>
